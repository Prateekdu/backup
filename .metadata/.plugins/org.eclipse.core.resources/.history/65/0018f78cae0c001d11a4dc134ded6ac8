package com;
public class rotationArray{
	public static void main(String args[])
	{
		int[] a= {11,22,33,44,55,66,77};
		int r=1;
		int n=a.length;
		System.out.println("Array before rotation : ");
		for(int i=0;i<n;i++)
		{
			System.out.println(a[i]+" ");
		}
		System.out.println();
		System.out.println("Array agter rotation : ");
		for(int i=0;i<n;i++)
		{
			System.out.println(a[i]+" ");
		}
	}
}