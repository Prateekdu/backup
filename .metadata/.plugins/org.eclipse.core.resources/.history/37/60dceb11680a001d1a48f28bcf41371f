class MyThread extends Thread{
	public void run()
	{
		try
		{
			System.out.println("thread class");
		}
		catch(Exception e)
		{
			System.out.println(e);
		}
	}
	public class Thread
	{
		public static void main (String args[]) 
		{
			MyThread my=new MyThread();
			my.start();
		}
	}
}