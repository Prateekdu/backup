package mno;
abstract class A11{
	abstract void dis1();
	
}
class B1 extends A11{
	void dis1() {
		System.out.println("A class dis1 method override by b class");
	}
	void dis2() {
		System.out.println("b class dis2 method");
	}
}
public class test2 {
	public static void main(String args[])
	{
		//A11 ob1=new A11();    we can create object of super class because it is abstract class
		B1 ob2=new B1(); 
		ob2.dis1();
		ob2.dis2();
		A11 ob3=new B1();
		ob3.dis1();
		ob3.dis2();
		
		
	}

}
