package code;
class A{
	void dis1()
	{
		System.out.println("A class dis1 method");
	}
}
class B extends A{
	void dis1() {
		System.out.println("A class dis1 method override by b class");
	}
	void dis2() {
		System.out.println("b class dis2 method");
	}
}
public class test1 {
	public static void main(String args[])
	{
		A ob1=new A();	
		ob1.dis1();
		B ob2=new B();  
		ob2.dis1();  
		ob2.dis2();
//		a obj4=new b();
	}

}
