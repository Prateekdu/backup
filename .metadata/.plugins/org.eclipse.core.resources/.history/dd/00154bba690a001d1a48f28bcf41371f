class thrd implements runnable{
	public void run() {
		try {
			System.out.println("hi");
		}
		catch(Exception e)
		{
			System.out.println("jack");
		}
	}
}
public class MyRunnableThread{
	public static void main(String args[])
	{
		MyRunnableThread mt=new MyRunnableThread();
		mt.start();
	}
}